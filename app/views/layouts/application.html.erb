<!DOCTYPE html>
<html>
	<%= render 'layouts/head' %>
<body class="<%= controller_name %> <%= action_name %>">
		<%= render 'layouts/header' %>
		<main role="main">
			<div class="container">
				<%= render 'layouts/flash' %>
			  <%= yield %>
		  </div>
		</main>
		<%= render 'layouts/footer' %>
		<%= debug(params) if Rails.env.development? %>
</body>
</html>
