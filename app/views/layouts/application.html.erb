<!DOCTYPE html>
<html>
	<%= render 'layouts/head' %>
<body class="<%= controller_name %> <%= action_name %>">
	<div class="wrapper-for-content-outside-of-footer">
		<%= render 'layouts/header' %>
		<main role="main">
			<div class="container">
				<%= render 'layouts/flash' %>
			  <%= yield %>
		  </div>
		</main>
	</div>
		<%= render 'layouts/footer' %>
		<%= debug(params) if Rails.env.development? %>
</body>
</html>
